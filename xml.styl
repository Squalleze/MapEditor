// out: $1.css, compress: true
*
  box-sizing: border-box
  margin: 0
  border-spacing: 0
  padding: 0

a, a:visited, a:hover, a:active
  color: inherit
  text-decoration: none

body
  font-family: 'Roboto'
  font-weight: 400

  color: #C2C2DA
  background-color: #324650

table
.table
  tr
  .tr
    td
    .a
      padding: 3px 5px
    &:nth-child(2n+1)
      background-color: rgba(0, 0, 0, .1)

fieldset
  overflow: hidden
  overflow: -moz-hidden-unscrollable

  margin-top: 5px
  border: none
  border-radius: 5px
  background-color: #465a6e

  legend
    // width: 100%
    border-radius: 5px
    padding: 3px 5px
    background-color: #465a6e
  label
    display: flex
    align-items: center

    input
      margin-right: 5px

.a:first-child
  box-sizing: content-box
.a
  text-align: left
  display: table-cell
  width: -moz-fit-content
  white-space: nowrap
  overflow: hidden
.table
  display: table
  width: 100%
.tr
  display: table-row
.del
  text-align: center
  width: -moz-fit-content
input
  // display: initial
  border: none
  background: none
  color: inherit

.bs-default
  box-sizing: initial
